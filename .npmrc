package-manager=npm
engine-strict=true

# Faster npm installs
fetch-retries=2
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=30000
prefer-offline=true
audit=false
fund=false
save-exact=true
progress=false
loglevel=error

# Use faster registry
registry=https://registry.npmjs.org/

# Disable package locks update
package-lock=false

# Increase network concurrency
network-concurrency=50
maxsockets=30 