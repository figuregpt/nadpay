# Next.js Environment Variables
NEXT_PUBLIC_APP_URL=https://nadpay.xyz

# WalletConnect Project ID (optional - for WalletConnect support)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here 

# Twitter OAuth Configuration
X_API_KEY=your_twitter_api_key
X_API_SECRET=your_twitter_api_secret
X_CALLBACK_URI=https://nadpay.xyz/api/auth/twitter/callback

# Contract Deployment (for development/deployment only)
PRIVATE_KEY=your_private_key_for_deployment_without_0x_prefix

# Optional: Database connection (for analytics/caching)
MONGODB_URI=mongodb://localhost:27017/nadpay

# Optional: IPFS configuration
IPFS_API_URL=https://ipfs.infura.io:5001
IPFS_API_KEY=your_ipfs_api_key
IPFS_API_SECRET=your_ipfs_api_secret 